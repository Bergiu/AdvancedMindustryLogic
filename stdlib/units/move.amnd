import ../simple_operations.amnd

function move_until(x, y, range) {
    exec move(x, y)
    exec within(x, y, range)
    while not within_out_move_is_within {
        exec within(x, y, range)
    }
}

function move_to_core_until(enemy, range) {
    exec locate_core(false)
    exec move_until(locate_core_out_x, locate_core_out_y, range)
}
