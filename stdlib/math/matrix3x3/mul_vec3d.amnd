import ./__struct__.amnd
import ../vec3d/__struct__.amnd

#             | x
#             | y
#             | z
# ------------|---
# a11 a12 a13 | x
# a21 a22 a23 | y
# a31 a32 a33 | z
function Matrix3x3::mul_vec3d(Matrix3x3 m1, Vec3D vec, Vec3D *out) {
    out.x = m1.a11 * vec.x + m1.a12 * vec.y + m1.a13 * z
    out.y = m1.a21 * vec.x + m1.a22 * vec.y + m1.a23 * z
    out.z = m1.a31 * vec.x + m1.a32 * vec.y + m1.a33 * z
}
